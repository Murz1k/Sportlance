{
  "name": "sportlance",
  "version": "0.1.7",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4900",
    "start-mobile": "ng serve --host 0.0.0.0",
    "build": "ng build --prod && gzipper ./output/dist -r",
    "build-dev": "ng build --configuration=development && gzipper ./output/dist -r",
    "deploy-prod": "npm run build & npm run prod-deploy & npm run prod-invalidate rd /S /Q dist",
    "prod-deploy": "prod-aws-s3-deploy.bat",
    "prod-invalidate": "prod-aws-cloudfrond-invalidate.bat",
    "lint": "ng lint",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server",
    "build:client-and-server-bundles": "ng build --prod && ng run sportlance:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "preversion": "",
    "version": "npm run build-dev",
    "postversion": "rd /S /Q dist"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-optimizer": "^0.7.2",
    "@angular/animations": "^7.1.2",
    "@angular/common": "^7.1.2",
    "@angular/compiler": "^7.1.2",
    "@angular/core": "^7.1.2",
    "@angular/forms": "^7.1.2",
    "@angular/http": "^7.1.2",
    "@angular/material": "^7.1.1",
    "@angular/platform-browser": "^7.1.2",
    "@angular/platform-browser-dynamic": "^7.1.2",
    "@angular/platform-server": "^7.2.4",
    "@angular/router": "^7.1.2",
    "@nguniversal/express-engine": "^7.1.0",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.0",
    "angular-moment": "^1.2.0",
    "core-js": "^2.6.0",
    "gzipper": "^1.4.2",
    "jwt-decode": "latest",
    "ngx-device-detector": "^1.3.5",
    "ngx-infinite-scroll": "^6.0.1",
    "ngx-moment": "^3.1.0",
    "node-sass": "^4.11.0",
    "popper.js": "^1.14.3",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "ts-loader": "^4.5.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.11.2",
    "@angular/cdk": "^7.1.1",
    "@angular/cli": "^7.1.2",
    "@angular/compiler-cli": "^7.1.2",
    "@angular/language-service": "^7.1.2",
    "@angular/material": "^7.1.1",
    "@types/jasmine": "^3.3.0",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^10.12.12",
    "codelyzer": "^4.5.0",
    "jasmine-core": "^3.3.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.1.3",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "^5.4.1",
    "rxjs-tslint": "^0.1.6",
    "ts-node": "~4.1.0",
    "tslint": "^5.11.0",
    "typescript": "^3.2.4",
    "webpack-cli": "^3.2.3"
  }
}
